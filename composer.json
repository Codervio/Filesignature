  "description": "Codervio Filesignature",
  "keywords": ["file",  "filesignature", "mime", "files", "php", "framework"],
  "homepage": "http://codervio.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Marin Sagovac",
      "email": "marin@sagovac.com"
    },
    {
      "name": "Codervio Community",
      "homepage": "http://codervio.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "codervio/filesignature": "1.*"
